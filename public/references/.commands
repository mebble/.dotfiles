# git
git diff --staged
git diff main  # current branch tip vs current (local) main tip
git diff main...HEAD  # current branch tip vs branch point (branch's base)
git diff main -- <filepath>
git diff main --name-only
git diff main --name-status
git diff origin/main # current branch to remote (since last fetch) main
git diff origin/main...HEAD # see exactly what your PR on github will show
git push origin main~1:main # push only upto parent commit
git remote rename origin upstream
git remote prune origin # remove remote branches that no longer exist on remote
git branch -vv
git branch master -u origin/master # similar to --set-upstream
git branch -D <branch> # delete branch locally
git push origin --delete <branch> # delete branch remotely. Run after deleting locally
git commit --amend --no-edit # amend without message
git commit --amend -m
git commit --allow-empty -m "trigger CI"
git config --global push.default current
git reset --hard HEAD~1 # move current branch back by 1 commit
git reset --soft HEAD~1 # move current branch back by 1 commit but keep changes in staging area
git stash push --staged # stash only staged changes
git stash push --keep-index # stash staged and unstaged changes, but keep staged changes in staging area
git revert --no-commit <commit-hash> # revert a commit without creating a new commit, just put changes in staging area
git revert --continue # when in revert mode, continue
git revert --abort # when in revert mode, abort
git submodule update --init --recursive # initialize all submodules
git worktree add ../<path> # new branch has same name as path
git worktree add ../<path> <branch> # worktree from branch that exists locally
git worktree add -b <new-branch> ../<path> # new branch is explicitly named. new branch created off of current branch/commit
git worktree add -b <new-branch> ../<path> origin/main # new branch created off of origin/main (not local main)
git worktree list
git worktree remove <worktree>
git fetch origin && git reset --hard origin/<branch> # fetch remote changes of branch, discarding the local changes in case the two have diverged
git checkout origin/main -- <filepath> # reset a file from the latest remote main
git checkout main -- <filepath> # reset a file from the latest local main
git clean -n <filepath> # delete untracked files (dry run)
git clean -f <filepath> # actually delete untracked files

